//A 강조하기
//https://school.programmers.co.kr/learn/courses/30/lessons/181874
function solution(myString) {
		var myString = prompt("입력"); 
		console.log(myString);
    myString.toLowerCase(); //문자열 소문자 변환
    var answer = myString.replaceAll("a", "A");
    console.log(answer);
    return myString;
} 

solution();
